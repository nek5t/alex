{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "alexblocks/details",
	"version": "0.1.0",
	"title": "Details Disclosure",
	"category": "text",
	"icon": "smiley",
	"description": "A summary with expandable detailed content.",
	"supports": {
		"html": false
	},
	"attributes": {
		"summary": {
			"type": "string",
			"source": "html",
			"selector": "h3"
		}
	},
	"textdomain": "alexblocks",
	"editorScript": "file:./index.js",
	"style": "file:./index.css"
}
